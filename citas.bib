@Article{Bourgain-2008,
  author      = {Jean Bourgain and Nataša Pavlović},
  title       = {Ill-posedness of the {N}avier-{S}tokes equations in a critical space in 3{D}},
  abstract    = {We prove that the Cauchy problem for the three dimensional Navier-Stokes equations is ill posed in $\dot{B}^{-1,\infty}_{\infty}$ in the sense that a ``norm inflation'' happens in finite time. More precisely, we show that initial data in the Schwartz class $\mathcal{S}$ that are arbitrarily small in $\dot{B}^{-1, \infty}_{\infty}$ can produce solutions arbitrarily large in $\dot{B}^{-1, \infty}_{\infty}$ after an arbitrarily short time. Such a result implies that the solution map itself is discontinuous in $\dot{B}^{-1, \infty}_{\infty}$ at the origin.},
  date        = {2008-07-06},
  eprint      = {http://arxiv.org/abs/0807.0882v1},
  eprintclass = {math.AP},
  eprinttype  = {arXiv},
  keywords    = {math.AP},
}

